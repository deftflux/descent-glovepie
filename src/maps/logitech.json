{
    "alias": {
        "DPadLeft": "(Joystick1.x <= -0.5)",
        "DPadRight": "(Joystick1.x >= 0.5)",
        "DPadUp": "(Joystick1.y <= -0.5)",
        "DPadDown": "(Joystick1.y >= 0.5)",

        "ActionLeft": "Joystick1.Button1",
        "ActionRight": "Joystick1.Button3",
        "ActionUp": "Joystick1.Button4",
        "ActionDown": "Joystick1.Button2",

        "LeftUpperShoulder": "Joystick1.Button5",
        "LeftLowerShoulder": "Joystick1.Button7",
        "RightUpperShoulder": "Joystick1.Button6",
        "RightLowerShoulder": "Joystick1.Button8",
        
        "LeftThumb": "Joystick1.Button11",
        "RightThumb": "Joystick1.Button12",

        "StartLeft": "Joystick1.Button9",
        "StartRight": "Joystick1.Button10",
    },

    "shift": [
        "StartLeft",
        "StartRight"
    ],

    "button": {
        "ActionDown": {
            "normal": "Keyboard.Enter",
            "StartLeft": "~Six",
            "StartRight": "~Alt-F1"
        },
        "ActionRight": {
            "normal": "Keyboard.NUMPAD6",
            "StartLeft": "~Seven",
            "StartRight": "~Alt-F3"
        },
        "ActionLeft": {
            "normal": "Keyboard.Space",
            "StartLeft": "~Nine",
            "StartRight": "~F2"
        },
        "ActionUp": {
            "normal": "Keyboard.NUMPAD8",
            "StartLeft": "~Eight",
            "StartRight": "~Tab"
        },
        "DPadUp": {
            "normal": "Keyboard.Up",
            "StartRight": "~Three"
        },
        "DPadDown": {
            "normal": "Keyboard.Down",
            "StartRight": "~One"
        },
        "DPadLeft": {
            "normal": "Keyboard.Left",
            "StartRight": "~Two",
            "StartLeft": "~Shift-F1"
        },
        "DPadRight": {
            "normal": "Keyboard.Right",
            "StartRight": "~Four",
            "StartLeft": "~Shift-F2"
        },
        "RightUpperShoulder": "Keyboard.NUMPAD9",
        "RightLowerShoulder": "Keyboard.NUMPAD3",
        "LeftThumb": "Keyboard.NUMPAD0",
        "RightThumb": "Keyboard.NumpadDot",
        "StartLeft": {
            "StartRight": "~Five"
        },
        "StartRight": {
            "normal": "~Escape",
            "StartLeft": "~Zero"
        }
    },

    "throttle": {
        "forwardButton": "Keyboard.NUMPAD7",
        "reverseButton": "Keyboard.NUMPAD1",
        "button": "Keyboard.Apostrophe",
        "forwardDigital": "LeftLowerShoulder",
        "reverseDigital": "LeftUpperShoulder"
    }
}